{
  "name": "frzr",
  "version": "0.3.0",
  "description": "Cool new javascript framework",
  "main": "dist/frzr.js",
  "scripts": {
    "dev": "node watch",
    "prepublish": "mkdir -p dist && npm run build-js",
    "build-js": "esperanto -s -b -t umd -n frzr -i lib/index.js | babel --blacklist strict -o dist/frzr.js && npm run uglify-js",
    "uglify-js": "uglifyjs dist/frzr.js -cm -o dist/frzr.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frzrjs/frzr"
  },
  "author": "Juha Lindstedt",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.21",
    "esperanto": "^0.7.4",
    "uglify-js": "^2.4.24"
  },
  "devDependencies": {
    "chokidar": "^1.0.5"
  }
}
